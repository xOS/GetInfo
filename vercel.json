{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.5.1",
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/(\\w*)\\.(?:css|jpeg|png|gif|bmp|webp)",
            "dest": "/api/style.css"
        }
    ]
}
